<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ch.customermgr.dao.CustomerDAO">
   
    <insert id="create" parameterType="com.ch.customermgr.model.CustomerModel">
        INSERT INTO tbl_customer (customerId,pwd,showName,trueName,registerTime)
        VALUES (#(customerId),#(pwd),#(showName),#(trueName),#(registerTime))
    </insert>
    <update id="update" parameterType="com.ch.customermgr.model.CustomerModel">
        UPDATE tbl_customer SET customerId=#(customerId),pwd=#(pwd),showName=#(showName),trueName=#(trueName),registerTime=#(registerTime)
      where uuid=#(uuid)
    </update>
    <update id="update"></update>
    <delete id="delete"></delete>
    
    <update id="updateNameById"></update>
</mapper>